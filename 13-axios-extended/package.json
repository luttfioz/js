{
  "name": "invoker",
  "version": "2.0.0",
  "description": "ESM Invoker Module",
  "module": "dist/index.esm.js",
  "main": "dist/index.esm.js",
  "files": [
    "dist",
    "public",
    "src",
    "test",
    ".babelrc",
    "rollup.config.js"
  ],
  "scripts": {
    "start": "npm run dev:js-es | npm run dev:js-node",
    "dev:js-es": "rollup -c -w --environment BUILD:development --environment ISNODE:false",
    "dev:js-node": "rollup -c -w --environment BUILD:development --environment ISNODE:true",
    "build:dev": "rollup -c --environment BUILD:dev",
    "build:prod": "npm run build:prod-es | npm run build:prod-node",
    "build:prod-es": "rollup -c --environment BUILD:production --environment ISNODE:false",
    "build:prod-node": "rollup -c --environment BUILD:production --environment ISNODE:true",
    "clean": "rimraf dist/*.js",
    "debug": "node --inspect ./src/index.js",
    "eslint": "eslint --fix . --ignore-path .gitignore",
    "test": "npm run nyc:coverage && npm run nyc:report",
    "test:debug": "mocha --inspect --watch -r esm",
    "test:coverage": "npm test > COVERAGE.md",
    "posttest:coverage": "node -e \"require('../../nyc.config.js').updateCoverage()\"",
    "nyc:coverage": "nyc --silent --temp-dir=test/test-reports/.nyc_output mocha -r esm",
    "nyc:report": "nyc report --reporter=html --reporter=text --reporter=lcov --report-dir=test/test-reports/coverage --temp-dir=test/test-reports/.nyc_output"
  },
  "author": ">",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "chai": "^4.2.0",
    "esm": "3.2.25",
    "eslint": "^6.4.0",
    "mocha": "^6.2.0",
    "mochawesome": "4.1.0",
    "nock": "11.3.5",
    "nyc": "^15.0.1",
    "rimraf": "2.6.3",
    "rollup": "1.19.3",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-gzip": "2.2.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-serve": "1.0.1",
    "rollup-plugin-size-snapshot": "0.10.0",
    "rollup-plugin-terser": "5.1.1",
    "sinon": "^7.5.0"
  }
}
